{
	"name": "patients-api",
	"version": "1.0.0",
	"description": "API GraphQL para b√∫squeda de pacientes",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"build": "tsc",
		"test": "jest",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
